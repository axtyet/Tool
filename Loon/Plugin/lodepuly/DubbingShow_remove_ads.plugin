[Rewrite]
# 首页 - 底部弹窗
^https:\/\/iosapi\.peiyinxiu\.com\/Api\/Film\/GetConfigValue reject-dict

# 首页 - 推荐 - 语聊弹窗
^https:\/\/iosapi\.peiyinxiu\.com\/Api\/Live\/GetExitRecommendLive reject-dict
^https:\/\/iosapi\.peiyinxiu\.com\/Api\/Live\/GetRecommendLive reject-dict

# 首页 - 推荐 - 关注推荐
^https:\/\/iosapi\.peiyinxiu\.com\/Api\/Friend\/GetRecommendUsers reject-dict

# 首页 - 推荐 - 推荐群聊
^https:\/\/iosapi\.peiyinxiu\.com\/api\/Union\/HomeData reject-dict

# 关注推荐
^https:\/\/iosapi\.peiyinxiu\.com\/api\/live\/FollowList reject-dict

[Script]
# 移除语聊横幅
http-response ^https:\/\/iosapi\.peiyinxiu\.com\/Api\/live\/LiveList script-path = https://raw.githubusercontent.com/axtyet/Tool/main/Loon/Script/lodepuly/DubbingShow/DubbingShow_remove_ads.js, requires-body = true, tag = 移除语聊横幅

[Mitm]
hostname = iosapi.peiyinxiu.com