[Rewrite]
# 开屏广告
^https:\/\/(qiye\.gaoding|www\.gaoding)\.com\/api\/v3\/oc\/v2\/delivery-pits\/ios-splash\/modules$ reject-dict

# 模板中心弹窗
^https:\/\/(qiye\.gaoding|www\.gaoding)\.com\/api\/v3\/oc\/delivery-pits\/template-page-dialog\/modules$ reject-dict

[Script]
# 移除首页轮播图
http-response ^https:\/\/(qiye\.gaoding|www\.gaoding)\.com\/api\/v3\/oc\/exhibitions\/template\/resources$ script-path = https://raw.githubusercontent.com/axtyet/Tool/main/Loon/Script/lodepuly/GaoDing/GaoDing_remove_ads.js, requires-body = true, tag = 移除首页轮播图

[Mitm]
hostname = qiye.gaoding.com, www.gaoding.com