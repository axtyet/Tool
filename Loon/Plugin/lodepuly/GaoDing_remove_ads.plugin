[Rewrite]
# 开屏广告
^https:\/\/www\.gaoding\.com\/api\/v\d\/oc\/v\d\/delivery-pits\/ios-splash\/modules$ reject-dict

# 模板中心弹窗
^https:\/\/www\.gaoding\.com\/api\/v\d\/oc\/delivery-pits\/(template|mine|home)-page-dialog\/modules$ reject-dict

# 搜索栏填充词
^https:\/\/www\.gaoding\.com\/api\/v3\/cp\/search-words\/v2\/placeholder reject-dict

# 移除首页轮播图
^https:\/\/www\.gaoding\.com\/api\/v3\/oc\/exhibitions\/template\/resources$ response-body-json-del pits[0].delivery_materials pits[2].delivery_materials

# 我的页面轮播图
^https:\/\/www\.gaoding\.com\/api\/v3\/oc\/exhibitions\/app_mine\/resources$ response-body-json-del pits[2].delivery_materials

[Mitm]
hostname = www.gaoding.com