[Rewrite]
# 开屏广告
^https:\/\/www\.gaoding\.com\/api\/v\d\/oc\/v\d\/delivery-pits\/ios-splash\/modules$ reject-dict

# 模板中心弹窗
^https:\/\/www\.gaoding\.com\/api\/v\d\/oc\/delivery-pits\/template-page-dialog\/modules$ reject-dict

# 搜索栏填充词
^https:\/\/www\.gaoding\.com\/api\/v3\/cp\/search-words\/v2\/placeholder reject-dict

[Script]
# 移除首页轮播图
http-response ^https:\/\/www\.gaoding\.com\/api\/v\d\/oc\/exhibitions\/template\/resources$ script-path = https://raw.githubusercontent.com/axtyet/Tool/main/Loon/Script/lodepuly/GaoDing/GaoDing_remove_ads.js, requires-body = true, tag = 移除首页轮播图
# 我的页面轮播图
http-response ^https:\/\/www\.gaoding\.com\/api\/v3\/oc\/exhibitions\/app_mine\/resources script-path = https://raw.githubusercontent.com/axtyet/Tool/main/Loon/Script/lodepuly/GaoDing/GaoDing_remove_ads.js, requires-body = true, tag = 移除我的页面轮播图

[Mitm]
hostname = www.gaoding.com